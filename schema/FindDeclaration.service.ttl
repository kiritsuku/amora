@prefix service: <http://amora.center/kb/Schema/Service/>
@prefix registry: <http://amora.center/kb/Service/>
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>

registry:FindDeclaration
  service:name "amora.backend.services.FindDeclaration" ;
  service:method [
    service:name "run" ;
    service:param [
      service:name "offset" ;
      a xsd:integer ;
    ] ;
    a xsd:string ;
  ] ;
.

